{
  "name": "my-hana-app-01",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^5",
    "@sap/hana-client": "^2.6.61",
    "@sap/hdbext": "^7.2.0",
    "express": "^4",
    "hdb": "^0.18.3",
    "sap-hdbext-promisfied": "^2.202105.2"
  },
  "scripts": {
    "start": "cds run"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "es2020": true,
      "node": true,
      "jest": true,
      "mocha": true
    },
    "globals": {
      "SELECT": true,
      "INSERT": true,
      "UPDATE": true,
      "DELETE": true,
      "CREATE": true,
      "DROP": true,
      "CDL": true,
      "CQL": true,
      "CXL": true,
      "cds": true
    },
    "rules": {
      "no-console": "off",
      "require-atomic-updates": "off"
    }
  },
  "cds": {
    "build": {
      "target": "."
    },
    "hana": {
      "deploy-format": "hdbtable"
    },
    "requires": {
      "db": {
        "kind": "hana"
      }
    }
  },
  "sapux": [
    "app/interaction_items",
    "app/frontend_01"
  ],
  "devDependencies": {
    "@sap/hdi-deploy": "4.0.5",
    "@sap/ux-specification": "^1.90.1"
  }
}
